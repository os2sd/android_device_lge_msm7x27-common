#/mount point	fstype	device			[device2]		[options]
/boot		mtd	boot
/cache		yaffs2	cache
/data		auto	/dev/block/mmcblk0p4
/misc		mtd	misc
/recovery	mtd	recovery					flags=backup=1
/system		auto	/dev/block/mmcblk0p2

/sdcard		vfat    /dev/block/mmcblk0p1	/dev/block/mmcblk0	flags=display="Micro SDcard";storage;wipeingui;removable
